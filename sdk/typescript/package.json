{
  "name": "circuit-breaker-sdk",
  "version": "0.1.0",
  "description": "Simple TypeScript SDK for Circuit Breaker workflow engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run clean && npm run build",
    "example:basic": "npx tsx examples/basic-usage.ts",
    "example:multi-provider": "npx tsx examples/multi-provider-demo.ts",
    "example:smart-routing": "npx tsx examples/smart-routing-demo.ts",
    "example:sse-streaming": "npx tsx examples/sse-streaming-demo.ts",
    "examples": "npm run example:basic"
  },
  "keywords": [
    "workflow",
    "automation",
    "circuit-breaker",
    "typescript",
    "sdk"
  ],
  "author": "Circuit Breaker Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/circuit-breaker/sdk.git",
    "directory": "typescript"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
