{
  "name": "circuit-breaker-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Circuit Breaker workflow engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./workflow": "./dist/workflow/index.js",
    "./resources": "./dist/resources/index.js",
    "./rules": "./dist/rules/index.js",
    "./functions": "./dist/functions/index.js",
    "./llm": "./dist/llm/index.js",
    "./agents": "./dist/agents/index.js",
    "./utils": "./dist/utils/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dev": "tsx src/examples/basic-workflow.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "docs": "typedoc",
    "prepublishOnly": "npm run clean && npm run build",
    "example:basic": "tsx examples/basic-workflow.ts",
    "example:workflow": "tsx examples/workflow-management.ts",
    "example:rules": "tsx examples/rules-demo.ts",
    "example:functions": "tsx examples/function-chains.ts",
    "example:llm": "tsx examples/llm-integration.ts",
    "example:agents": "tsx examples/ai-agent.ts"
  },
  "keywords": [
    "workflow",
    "automation",
    "state-machine",
    "graphql",
    "ai",
    "llm",
    "docker",
    "functions",
    "circuit-breaker",
    "typescript"
  ],
  "author": "Circuit Breaker Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/circuit-breaker/sdk.git",
    "directory": "typescript"
  },
  "bugs": {
    "url": "https://github.com/circuit-breaker/sdk/issues"
  },
  "homepage": "https://github.com/circuit-breaker/sdk#readme",
  "dependencies": {
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "ws": "^8.14.2",
    "graphql-ws": "^5.14.2",
    "uuid": "^9.0.1",
    "cross-fetch": "^4.0.0",
    "eventsource": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.9",
    "@types/uuid": "^9.0.7",
    "@types/eventsource": "^1.1.15",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "prettier": "^3.1.0",
    "typedoc": "^0.25.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  }
}
